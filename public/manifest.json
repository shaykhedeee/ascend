{
  "name": "Ascendify - AI Habit Tracker & Goal Planner",
  "short_name": "Ascendify",
  "description": "Build lasting habits with AI-powered goal decomposition. Track daily habits, earn XP, level up, and achieve your goals with gamified progress tracking. Free habit tracker with analytics.",
  "start_url": "/?source=pwa",
  "id": "/ascendify-app",
  "scope": "/",
  "display": "standalone",
  "display_override": ["standalone", "minimal-ui"],
  "background_color": "#0A0A0B",
  "theme_color": "#14B899",
  "orientation": "portrait-primary",
  "lang": "en-US",
  "dir": "ltr",
  "categories": ["productivity", "lifestyle", "health", "fitness", "education"],
  "icons": [
    {
      "src": "/icons/icon.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "/icons/icon.svg",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "maskable"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/dashboard.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Ascendify Dashboard"
    },
    {
      "src": "/screenshots/mobile.png",
      "sizes": "390x844",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Ascendify Mobile View"
    }
  ],
  "shortcuts": [
    {
      "name": "Dashboard",
      "short_name": "Home",
      "description": "View your progress dashboard",
      "url": "/?tab=dashboard&source=shortcut",
      "icons": [{"src": "/icons/icon.svg", "sizes": "96x96"}]
    },
    {
      "name": "Track Habits",
      "short_name": "Habits",
      "description": "Check off your daily habits",
      "url": "/?tab=habits&source=shortcut",
      "icons": [{"src": "/icons/icon.svg", "sizes": "96x96"}]
    },
    {
      "name": "View Goals",
      "short_name": "Goals",
      "description": "Check your goal progress",
      "url": "/?tab=goals&source=shortcut",
      "icons": [{"src": "/icons/icon.svg", "sizes": "96x96"}]
    },
    {
      "name": "Quick Add",
      "short_name": "Add",
      "description": "Quickly add a new task",
      "url": "/?action=quickadd&source=shortcut",
      "icons": [{"src": "/icons/icon.svg", "sizes": "96x96"}]
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "share_target": {
    "action": "/?action=share",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+ascendify",
      "url": "/?action=%s"
    }
  ]
}
